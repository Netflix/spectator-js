{
  "name": "nflx-spectator",
  "version": "2.0.0",
  "author": "Matthew Johnson <matthewj@gmail.com>",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "homepage": "https://github.org/Netflix/spectator-js",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:Netflix/spectator-js.git"
  },
  "engines": {
    "node": ">6.4.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "coveralls": "^3.1.0",
    "eslint": "^8.41.0",
    "express": "^4.18.2",
    "jscs": "^3.0.7",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "tsd": "^0.28.1"
  },
  "scripts": {
    "clean": "rm -f ./src/*.d.ts",
    "copy": "cp -rf ./types/* ./src",
    "prepare": "npm run clean && npm run copy",
    "test": "npm run test:unit && npm run test:types",
    "test:types": "npm run clean && npm run copy && tsd",
    "test:unit": "mocha --reporter spec",
    "cover": "node_modules/.bin/nyc node_modules/mocha/bin/_mocha test/*",
    "lint": "eslint src/*.js test/*.js"
  },
  "files": [
    "src/*",
    "src/index.d.ts"
  ],
  "tsd": {
    "directory": "test-d",
    "compilerOptions": {
      "esModuleInterop": true
    }
  },
  "dependencies": {
    "async": "^3.1.1",
    "needle": "^3.2.0"
  }
}
